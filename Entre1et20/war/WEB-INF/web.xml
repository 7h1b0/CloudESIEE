<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Post</servlet-name>
		<servlet-class>com.example.entre1et20.Post</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Post</servlet-name>
		<url-pattern>/post</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Calcul</servlet-name>
		<servlet-class>com.example.entre1et20.Calcul</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Calcul</servlet-name>
		<url-pattern>/calcul</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Add</servlet-name>
		<servlet-class>com.example.entre1et20.Add</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Add</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Winner</servlet-name>
		<servlet-class>com.example.entre1et20.Winner</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Winner</servlet-name>
		<url-pattern>/winner</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Restore</servlet-name>
		<servlet-class>com.example.entre1et20.Restore</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Restore</servlet-name>
		<url-pattern>/restore</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Initialisation</servlet-name>
		<servlet-class>com.example.entre1et20.Init</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Initialisation</servlet-name>
		<url-pattern>/init</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Play</servlet-name>
		<servlet-class>com.example.entre1et20.Play</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Play</servlet-name>
		<url-pattern>/play</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
